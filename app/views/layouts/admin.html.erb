	<script type="text/javascript" src="/javascripts/excanvas.js"></script>
<![endif]-->

<!-- Jquery and plugins -->

</head>
<body>
	
	<!-- Begin control panel wrapper -->
	<div id="wrapper">
	
		<!-- Begin top bar -->
		<div id="top_bar">
			
			
			<!-- Begin logo -->
			<div class="logo">
				<img src="/images/logos/64x64.png" alt="Logotype"/>
			</div>
			<!-- End logo -->
			
			<!-- Begin account menu -->
			<div class="account">
				<div class="detail">
					Hello <a href=""><strong>Admin</strong></a>
				</div>
				<ul class="icon">
					<li>
						<a href="" title="Message">
							<img src="/images/icon_message.png" alt="" class="middle"/>
						</a>
					</li>
					<li>
						<a href="" title="Setting">
							<img src="/images/icon_setting.png" alt="" class="middle"/>
						</a>
					</li>
					<li>
						<a href="/logout" title="Logout">
							<img src="/images/icon_logout.png" alt="" class="middle"/>
						</a>
					</li>
				</ul>
			</div>
			<!-- End account menu -->
			
			
		</div>
		<!--End top bar -->
		
		<!-- Begin main menu -->
		<div id="menu_wrapper">
		
			<ul class="nav">
				<li>
					<a href="/admin/dashboard" class="active">
						Home
					</a>
				</li>
				<li>
					<a href="">
						Stories
					</a>
					<div class="popup">
						<div class="top"></div>
						<div class="content">
							<ul class="submenu">
								<li>
									<a href="/admin/stories">
										All stories
									</a>
								</li>
								<li>
									<a href="/admin/stories/mine">
										My stories
									</a>
								</li>
								<li>
									<a href="/admin/stories/new">
										New story
									</a>
								</li>
							</ul>
							<br class="clear"/>
						</div>
						<div class="footer"></div>
					</div>
				</li>
				<li>
					<a href="">
						Users
					</a>
					<div class="popup">
						<div class="top"></div>
						<div class="content">
							<ul class="submenu">
								<li>
									<a href="">
										Add Users
									</a>
								</li>
								<li>
									<a href="">
										Manage Users
									</a>
								</li>
								<li>
									<a href="">
										Delete Users
									</a>
								</li>
							</ul>
							<br class="clear"/>
						</div>
						<div class="footer"></div>
					</div>
				</li>
			</ul>
			
			<!-- <div id="search">
				<form action="index.html" id="search_form" name="search_form" method="get">
					<p><input type="text" id="q" name="q" title="Search" class="search noshadow"/></p>
				</form>
			</div> -->
			
		</div>
		<!-- End main menu -->
		
		
		<!-- Begin shortcut menu -->
		<div class="shortcut">
			<div class="top"></div>
			<div class="content">
				<h2>Shortcut</h2>
			    <ul id="shortcut_item" class="items">
			    	<li>
			    		<a href="" title="Calendar">
			    			<img src="/example/icon_calendar.png" alt=""/>
			    		</a>
			    	</li>
			    	<li>
			    		<a href="" title="Contact">
			    			<img src="/example/icon_contact.png" alt=""/>
			    		</a>
			    	</li>
			    	<li>
			    		<a href="" title="Chart">
			    			<img src="/example/icon_chart.png" alt=""/>
			    		</a>
			    	</li>
			    	<li>
			    		<a href="" title="Note">
			    			<img src="/example/icon_note.png" alt=""/>
			    		</a>
			    	</li>
			    	<li>
			    		<a href="" title="Network">
			    			<img src="/example/icon_network.png" alt=""/>
			    		</a>
			    	</li>
			    </ul>
			    <br class="clear"/>
			</div>
			<div class="footer"></div>
		</div>
		<!-- End shortcut menu -->
		
		<br class="clear"/>
		
		<!-- Begin content -->
		<div id="content_wrapper">
			<%= yield %>
		</div>
		<!-- End content -->
		
		<br class="clear"/>
			
		<div id="footer">
			&copy; Copyright 2011 by Daniel Sundström
		</div>
	
	</div>
	<!-- End control panel wrapper -->
	<script type="text/javascript" src="/javascripts/jquery.js"></script>
	<script type="text/javascript" src="/javascripts/jquery-ui.js"></script>
	<script type="text/javascript" src="/javascripts/jquery.tipsy.js"></script>
	<script type="text/javascript" src="/javascripts/hint.js"></script>
	<script type="text/javascript" src="/javascripts/jquery.img.preload.js"></script>
	<script type="text/javascript" src="/javascripts/fancybox/jquery.fancybox-1.3.0.js"></script>
	<script type="text/javascript" src="/javascripts/custom.js"></script>
 <script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAA6CJnGpI-7SvVVlNcYelPwhRbqgL_ELVVW56uQtf2062fYZ3cDhQuAJNKrvn7_LdFiskjmX7BHOWgJA" type="text/javascript"></script>
	<%= yield :script_files %>

	<script type="text/javascript">
		<%= yield :page_script %>
	</script>
	
</body>
</html>